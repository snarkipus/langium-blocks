{
  "name": "blocks",
  "scopeName": "source.blocks",
  "fileTypes": [
    ".block"
  ],
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "name": "keyword.control.blocks",
      "match": "\\b(BOOKS|CARDS|category-ref|END|END-INSTRUCTIONS|EXECUTE|item-ref|QTY|SCENARIO|TOOLS|UAN|UAN-DEFINITION)\\b|\\b(DESC:|RANDOM-NUMBER-SEED:|ref:|TOOL-USAGE:)\\B"
    },
    {
      "name": "string.quoted.double.blocks",
      "begin": "\"",
      "end": "\""
    },
    {
      "name": "string.quoted.single.blocks",
      "begin": "'",
      "end": "'"
    }
  ],
  "repository": {
    "comments": {
      "patterns": []
    }
  }
}