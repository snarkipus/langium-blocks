{
  "name": "blocks",
  "scopeName": "source.blocks",
  "fileTypes": [
    ".block"
  ],
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "name": "keyword.control.blocks",
      "match": "\\b(UAN-DEFINITION)\\b|\\b(END-INSTRUCTIONS)\\b|\\b(BOOKS|CARDS|END|EXECUTE|Property|SCENARIO|TOOLS|UAN|SDB)\\b|\\b(RANDOM-NUMBER-SEED:)\\B"
    },
    {
      "name": "string.quoted.double.blocks",
      "begin": "\"",
      "end": "\""
    },
    {
      "name": "string.quoted.single.blocks",
      "begin": "'",
      "end": "'"
    }
  ],
  "repository": {
    "comments": {
      "patterns": []
    }
  }
}